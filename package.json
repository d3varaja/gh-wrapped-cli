{
  "name": "github-wrapped-2025",
  "version": "1.0.0",
  "description": "Your GitHub year in review - in your terminal",
  "main": "dist/index.js",
  "bin": {
    "github-wrapped": "./dist/index.js",
    "github-wrapped-2025": "./dist/index.js"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "bun run src/index.tsx",
    "build": "bun build src/index.tsx --outdir dist --target node --minify --external yoga-wasm-web",
    "postbuild": "node -e \"const fs = require('fs'); fs.cpSync('src/fonts', 'dist/fonts', {recursive: true}); fs.copyFileSync('src/export-worker.mjs', 'dist/export-worker.mjs');\"",
    "build:types": "tsc --emitDeclarationOnly",
    "start": "bun run dist/index.js",
    "prepublishOnly": "bun run build && bun run build:types",
    "test": "bun test"
  },
  "keywords": [
    "github",
    "wrapped",
    "cli",
    "terminal",
    "stats",
    "year-in-review",
    "github-stats",
    "developer-tools"
  ],
  "author": "d3varaja",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d3varaja/gh-wrapped-cli.git"
  },
  "dependencies": {
    "@octokit/graphql": "9.0.3",
    "@octokit/rest": "^20.0.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "date-fns": "^3.0.0",
    "dotenv": "^16.6.1",
    "ink": "^4.4.1",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "ora": "^7.0.1",
    "react": "^18.2.0",
    "satori": "0.18.3",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "react-devtools-core": "7.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "trustedDependencies": [
    "@resvg/resvg-js"
  ]
}
